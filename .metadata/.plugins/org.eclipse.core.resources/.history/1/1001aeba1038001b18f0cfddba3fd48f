package courseraita;

public class RankingPontos {

	String _usuario;
	String _pontos;

	public  RankingPontos (String usuario, String numeroPontos) {
		System.out.println("Ranking -> " + usuario + " , "  +  numeroPontos);
		this._usuario   = usuario.toLowerCase();
		this._pontos    = numeroPontos;
	}
	public String getUsuario () {		
		return _usuario;
	}
	public String getPontos () {		
		return _pontos; 
	}
}
